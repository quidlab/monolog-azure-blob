{
    "name": "quidlab/monolog-azure-blob",
    "description": "Azure Blob Storage Handler for Monolog forked from blue32a",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "quidlab",
            "email": "kamaldua@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "monolog/monolog": ">=3.2.0",
        "microsoft/azure-storage-blob": "^1.5"
    },
    "require-dev": {
        "quidlab/php-coding-standard": "^1.0",
        "mockery/mockery": "^1.3",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Quidlab\\Monolog\\Handler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "vendor/bin/parallel-lint --no-progress --exclude vendor .",
        "cs": "vendor/bin/phpcs",
        "cbf": "vendor/bin/phpcbf",
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
